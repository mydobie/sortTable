{
  "name": "@mydobie/sort-table",
  "version": "3.1.0",
  "main": "SortTable.js",
  "types": "SortTable.d.ts",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "https://github.com/mydobie/sortTable",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": "^14"
  },
  "dependencies": {
    "fastest-levenshtein": "^1.0.12",
    "lodash": "^4.17.21",
    "react": "16.8.0 - 17.999.999",
    "react-dom": "16.8.0 - 17.999.999"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/jest-axe": "^3.5.3",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "5.1.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-watch": "^7.0.0",
    "husky": "^7.0.2",
    "jest-axe": "^5.0.1",
    "lint-staged": "^11.2.2",
    "prettier": "2.4.1",
    "react-scripts": "^4.0.3",
    "sass": "1.39.2",
    "typescript": "^4.4.4",
    "uglify-js": "^3.14.2"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "set-value": "4.0.1",
    "immer": "9.0.6"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "d": "npm i",
    "installDependencies": "npm run d",
    "start": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts start",
    "----------------------------------------------------------1": "",
    "buildts": "tsc --project tsconfig-build-files.json",
    "build": "npm run d && npm run test && npm run npmAudit && npm run build:ci",
    "build:ci": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "react-scripts test --coverage  --passWithNoTests --testPathIgnorePatterns /helpers --watchAll=false ",
    "test:watch": "react-scripts test ",
    "test:update": "npm run test -- -u",
    "----------------------------------------------------------2": "",
    "lint": "npm run lint:js && npm run prettier:all",
    "lint:fix": "npm run prettier:fix:all && npm run lint:js:fix ",
    "lint:watch": "onchange 'src/**/*.*' -- npm run lint:fix",
    "----------------------------------------------------------3": "",
    "lint:js": "esw --ext .jsx,.js --color ",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier  --ignore-path ./.eslintignore --check",
    "prettier:all": "npm run prettier '**/*.+(js|jsx|html|css|scss|json|md)'",
    "prettier:fix": "prettier  --ignore-path ./.eslintignore --write",
    "prettier:fix:all": "npm run prettier:fix '**/*.+(js|jsx|html|css|scss|json|md)'",
    "----------------------------------------------------------4": "",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && npm run d",
    "uglify": "uglifyjs ",
    "preinstall": "npx npm-force-resolutions",
    "checkCoverage": "node ./utils/testCoverage.js",
    "npmAudit": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org",
    "buildPackage": "npm run d && npm run npmAudit && npm run test && bash  utils/buildPackage.sh"
  },
  "babel": {
    "presets": [
      "react",
      "env"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 80
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    },
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "globals": {
      "styles": true
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "jest": true,
      "es6": true,
      "jquery": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "prettier",
      "eslint-plugin-react"
    ],
    "rules": {
      "react/static-property-placement": "off",
      "prettier/prettier": "error",
      "no-var": "error",
      "curly": "error",
      "one-var": "error",
      "no-console": "error",
      "no-unused-vars": "error",
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run npmAudit"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/setupTests.js",
      "!<rootDir>/src/__tests__/helpers/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/App.tsx",
      "!<rootDir>/src/index.tsx"
    ],
    "coverageReporters": [
      "html",
      "json-summary",
      "text-summary"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
